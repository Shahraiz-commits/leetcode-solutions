class Solution(object):
    def longestCommonPrefix(self, strs):
        """
        :type strs: List[str]
        :rtype: str
        """
        prefix = strs[0]
        matches = False
        for str in strs:
            matches = False
            while(not matches):
                if(not str.startswith(prefix)):
                    prefix = prefix[:-1]
                else:
                    matches = True
        return prefix
                
                
        
